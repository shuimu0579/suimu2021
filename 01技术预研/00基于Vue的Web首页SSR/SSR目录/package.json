{
  "name": "yundee_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
      "serve": "vue-cli-service serve",
      "build": "vue-cli-service build",
      "lint": "vue-cli-service lint",
      
      "build:client": "vue-cli-service build",
      "build:ssr": "cross-env NODE_ENV=production WEBPACK_TARGET=node vue-cli-service build",
      "build:yundee": "npm run build:ssr && move dist\\vue-ssr-server-bundle.json bundle && npm run build:client && move bundle dist\\vue-ssr-server-bundle.json && cross-env WEBPACK_TARGET=node NODE_ENV=production node ./server/ssr.js",
      "start": "cross-env NODE_ENV=production node ./server/ssr.js",
      "serve:ssr": "cross-env WEBPACK_TARGET=node node ./server/ssr.js",
      "dev:yundee": "concurrently \"npm run serve\" \"npm run serve:ssr\" ",

      "new:comp": "node ./scripts/generateComponent/index",
      "new:view": "node ./scripts/generateView/index",
      "serves": "node ./scripts/multiProjectConfig/serve.js",
      "builds": "node ./scripts/multiProjectConfig/build.js"
  },
  "dependencies": {
      "three": "^0.118.3",
      "three-orbit-controls": "^82.1.0",
      "axios": "^0.19.0",
      "copy-webpack-plugin": "^5.1.1",
      "core-js": "^3.6.4",
      "element-ui": "^2.13.0",
      "koa": "^2.5.3",
      "koa-compress": "^3.0.0",
      "koa-mount": "^4.0.0",
      "koa-router": "^7.4.0",
      "koa-send": "^5.0.0",
      "koa-static": "^5.0.0",
      "qs": "^6.9.2",
      "swiper": "^5.3.8",
      "vue": "^2.6.12",
      "vue-awesome-swiper": "^4.1.1",
      "vue-router": "^3.1.6",
      "vuedraggable": "^2.24.3",
      "vuex": "^3.4.0",
      "weixin-js-sdk": "^1.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.3",
    "babel-plugin-component": "^1.1.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "lodash.merge": "^4.6.1",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-server-renderer": "2.5.17",
    "vue-template-compiler": "2.5.17",
    "webpack": "^4.20.2",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
/* pages/wxmininative/native.wxss */
page {
  background-color: #fff;
}
.container {
  padding: 0;
  background-color: #fff;
  height: 100%;
}

.header {
  width: 700rpx;
  height: 240rpx;
  padding: 24rpx;
  box-sizing: content-box;
}

.headerImg {
  width: 100%;
  height: 100%;
}

.headerIcon {
  width: 38rpx;
  height: 48rpx;
  display: inline-block;
  vertical-align: middle;
}

.headerRow {
  display: flex;
  padding: 60rpx;
  justify-content: space-between;
}

.headerText {
  font-size: 44rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 600;
  text-align: left;
  color: #fff;
}
.navHeader {
  padding: 0 32rpx;
  display: flex;
  justify-content: space-between;
}
.navTitle {
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 600;
  text-align: left;
  color: #333333;
}
.nav {
  padding: 50rpx 25rpx;
  width: 100%;
  box-sizing: border-box;
  /* min-height: 600rpx; */
  background: #fff;
  display: flex;
  justify-content: left;
  flex-wrap: wrap;
  align-content: space-between;
}
.navText {
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 300;
  margin: 0 auto;
  color: #333333;
  overflow: hidden;
  width: 150rpx;
  text-align: center;
}
.menuBox {
  width: 25%;
  height: 150rpx;
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 30rpx;
}

.title {
  width: 750rpx;
  height: 80rpx;
  background: #f8f8f8;
  text-align: center;
}

.title text {
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  text-align: center;
  color: #333;
  display: inline-block;
  font-size: 28rpx;
  vertical-align: -16rpx;
  padding: 0 40rpx;
}

.navImg {
  width: 80rpx;
  height: 80rpx;
}

.navIcon {
  width: 100rpx;
  height: 100rpx;
  border-radius: 12rpx;
  margin: 0 auto;
  background-size: cover;
}

.list {
  display: flex;
  width: 686rpx;
  justify-content: space-between;
  min-height: 90rpx;
  border-top: 2rpx solid #eee;
  box-sizing: border-box;
  padding: 24rpx 0;
  margin: auto;
}

.list:nth-child(2) {
  border-top: none;
}
.loading {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 88rpx;
  height: 200rpx;
  transform: translate(-50%, -50%);
  display: flex;
  flex-wrap: wrap;
}

.loading image{
  width: 80rpx;
  height: 80rpx;
}
.vanloading{
  margin: auto;
}
.footer {
  position: relative;
  margin: 10rpx 0rpx 20rpx 0rpx;
  width: 750rpx;
  height: 70rpx;
  background: #fff;
  padding: 0 32rpx;
  box-sizing: border-box;
  align-items: center;
  display: flex;
}

.footer .footerText {
  display: inline-block;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  text-align: left;
  color: #333;
}

.footer .rightIcon {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  right: 24rpx;
  top: 50%;
  transform: translateY(-50%);
}

.itemImg {
  width: 200rpx;
  height: 160rpx;
  position: relative;
}

.itemImg view {
  width: 40rpx;
  height: 40rpx;
  background: #295ffe;
  position: absolute;
  bottom: 16rpx;
  right: 16rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.itemImg text {
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #fff;
}

/* tab样式 */

.tab {
  display: flex;
  flex-direction: column;
}

.tab-nav {
  height: 80rpx;
  background: #fff;
  border-bottom: 0.5rpx dotted #ddd;
  display: flex;
  line-height: 79rpx;
  position: relative;
}

.tab-line {
  position: absolute;
  left: 0;
  bottom: -1rpx;
  height: 4rpx;
  background: #295ffe;
  transition: all 0.3s;
}

.tab-content {
  flex: 1;
  overflow-y: auto;
  overflow-x: hidden;
}

.scroll {
  white-space: nowrap;
  position: fixed;
  z-index: 999;
}

.hashinfo {
  color: #295ffe;
  display: inline-block;
  width: 55%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.empty-content {
  width: 100%;
  padding-top: 40rpx;
  height: 380rpx;
  position: relative;
}
.empty-image {
  text-align: center;
}

.empty-image image {
  width: 340rpx;
  height: 166rpx;
}
.empty-description {
  display: inline-block;
  height: 68rpx;
  font-size: 24rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: center;
  color: #999999;
  left: 50%;
  position: absolute;
  transform: translateX(-50%);
}
.empty-bottom {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
